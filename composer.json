{
    "name": "ivancli/alpha-vantage",
    "description": "This package provides methods to access alpha vantage APIs",
    "type": "library",
    "ketwords": ["alpha", "vantage", "finance", "api", "stock"],
    "homepage": "https://github.com/ivancli",
    "license": "MIT",
    "authors": [
        {
            "name": "Ivan Li",
            "email": "ivan.li@hotmail.com",
            "homepage": "https://github.com/ivancli"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.0",
        "guzzlehttp/guzzle": "^6.3",
        "illuminate/support": "^5.6",
        "laravel/laravel": "^5.6"
    },
    "autoload": {
        "psr-4": {
            "IvanCLi\\AlphaVantage\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IvanCLi\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "IvanCLi\\AlphaVantage\\ServiceProvider"
            ],
            "aliases": {
                "AlphaVantage": "IvanCLi\\AlphaVantage\\Facade"
            }
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "orchestra/testbench": "^3.6"
    }
}
